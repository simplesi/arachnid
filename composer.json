{
    "name": "codeguy/arachnid",
    "type": "library",
    "description": "A crawler to find all unique internal pages on a given website",
    "keywords": ["crawl","spider","search","scrape"],
    "homepage": "http://github.com/codeguy/arachnid",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "homepage": "http://www.joshlockhart.com/"
        },
        {
            "name": "Zeid Rashwani",
            "homepage": "http://zrashwani.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "fabpot/goutte": "^3.1",
        "davechild/textstatistics": "1.*",
        "google/apiclient": "^2.0",
        "doctrine/dbal": "2.5.*",
        "doctrine/orm": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-0": { "Arachnid": "src" }
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    }
}
